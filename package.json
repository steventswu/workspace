    "codebird": "^2.6.0",
    "ethjs": "^0.4.0",
    "highcharts": "^6.1.0",
    "i18next": "^11.3.6",
    "i18next-browser-languagedetector": "^2.2.2",
    "i18next-localstorage-backend": "^1.1.4",
    "idx": "^2.3.0",
    "is-webview": "^1.0.1",
    "lodash": "^4.17.4",
    "lodash-decorators": "^4.4.1",
    "moment": "^2.19.1",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.1.0",
    "prop-types": "^15.5.10",
