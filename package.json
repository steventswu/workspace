  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "1.11.1",
    "umi-plugin-routes": "^0.1.5"
  },
  "optionalDependencies": {
    "puppeteer": "^1.1.1"
